{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": {
        "laravel-master-password": {
            "type": "path",
            "url": "packages/aecor/laravel-master-password",
            "options": {
                "symlink": true
            }
        },
        "opta-sdapi-php": {
            "type": "path",
            "url": "packages/aecor/opta-sdapi-php",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^7.1.3",
        "aecor/laravel-master-password": "dev-upgrade",
        "aecor/opta-sdapi-php": "dev-upgrade",
        "aloha/twilio": "^4.0",
        "anlutro/l4-settings": "^0.8.1",
        "askedio/laravel5-profanity-filter": "^1.10",
        "barryvdh/laravel-snappy": "^0.4.4",
        "bensampo/laravel-enum": "^1.16",
        "beyondcode/laravel-websockets": "^1.1",
        "binarytorch/larecipe": "^2.0",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "doctrine/dbal": "^2.9",
        "elic-dev/laravel-site-protection": "^1.0",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "joggapp/laravel-mail-viewer": "^4.0",
        "kawankoding/laravel-fcm": "^0.1.3",
        "laracasts/flash": "^3.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/nexmo-notification-channel": "^2.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "riverskies/laravel-mobile-detect": "^1.3",
        "spatie/image-optimizer": "1.1.5",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-model-cleanup": "^2.2",
        "spatie/laravel-permission": "^2.25",
        "stuyam/laravel-phone-validator": "^1.0",
        "tightenco/ziggy": "^0.6.9",
        "twilio/sdk": "^5.28",
        "tymon/jwt-auth": "1.*",
        "watson/active": "^3.0",
        "worldpay/worldpay-lib-php": "^2.1",
        "yajra/laravel-datatables": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/telescope": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
